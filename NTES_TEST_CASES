		{
			System.out.println("Test case 63 Pass-More close successfully");
		}
		else
		{
			System.out.println("Test case 63 Fail-More option still display");
		}
		}
		catch(Exception e)
		{
			System.out.println("Test case 63 Fail-More option still display");
		}
	}

	private static void Google_click() {
		// TODO Auto-generated method stub
		try
		{
			Thread.sleep(3000);	
			driver.findElement(By.className("android.widget.ImageView")).click();
			Thread.sleep(4000);
			driver.findElement(By.id("cris.icms.ntes:id/radio")).click();
			System.out.println("Test case 61 Pass-User successfully click on Google ");
		}
		catch(Exception e)
		{
			System.out.println("Test case 62 Fail-Google button not found");
		}
	}

