package NITES_APP;

import java.net.MalformedURLException;
import java.net.URL;
import java.util.Scanner;

import org.openqa.selenium.By;
import org.openqa.selenium.remote.DesiredCapabilities;

import io.appium.java_client.AppiumDriver;
import io.appium.java_client.android.AndroidDriver;
import io.appium.java_client.remote.MobileCapabilityType;

public class NITES_RAILWAY_APP {
	private static final URL NULL = null;
	   static AppiumDriver driver;
	   static Scanner s=new Scanner(System.in);
	   public static void main(String[] args) throws InterruptedException
	   {
	   	
	   	   System.out.println("Launching app in device");	
	   	     DesiredCapabilities dc=new DesiredCapabilities();
	   	     dc.setCapability(MobileCapabilityType.AUTOMATION_NAME, "Appium");
	   	     dc.setCapability(MobileCapabilityType.PLATFORM_NAME, "Android");
	   	     dc.setCapability(MobileCapabilityType.DEVICE_NAME,"Galaxy J8" );
	   	     dc.setCapability(MobileCapabilityType.PLATFORM_VERSION, "10");
	   	  dc.setCapability("appPackage","cris.icms.ntes");
	   	 	dc.setCapability("appActivity","cris.icms.ntes.LaunchApp");
	   	 	dc.setCapability("noReset", "true");
	   	 URL server_url=NULL;
	   	 try
	   	 	{
	   	 	server_url= new URL("http://127.0.0.1:4723/wd/hub");
	   	 	System.out.println("Server connected");
	   	 	}
	   	 	catch(MalformedURLException e)
	   	 	{
	   	 	System.out.println("Server not found");	
	   	 	System.out.println(e.getMessage());
	   	 	}
	   	 	Thread.sleep(8000);
	   	   
	   	   driver=new AndroidDriver(server_url,dc);
	   	   dc.setCapability("newCommandTimeout", 10);
	   	   System.out.println("Creating Driver for android with all Desired capability and server URL");
	   		System.out.println("App open");
	   		System.out.println("1st screen start");
	   		//Test case 1-Verify NTES app should successfully launch
	   		ntes_launch();
	   		//Test case 2-Verify Notification icon should display on 1st screen
	   		notification_icon();
	   		//Test case 3- Verify notification icon should be clickable
	   		notification_click();
	   		//Test case 4-User should successfully click on Notification icon
	   		notification_userclick();
	   		//Test case 5-Verify user should navigates to next screen after click on Notification icon
	   		notification_navigation();
	   		//Test case 6-Verify Notification toggle button should display on screen
	   		notification_switch();
	   		//Test case 7-Verify user can switch notification option as On/Off
	   		notification_on_off();
	   		//Test case 8-Verify user should Turn ON notification
	   		notification_on();
	   		//Test case 9-Verify user should Turn OFF notification
	   		NOTIFICATION_OFF();
	   		//Test Case 10-Verify back button present on notification screen should clickable
	   		notification_backbtn();
	   		//Test case 11-Verify user should click on back button from notification screen
	   		notification_back_click();
	   		//Test case 12-Verify after click on back button , user should navigates to Home page
	   		notification_back_navigation();
	   		//Test case 13-Verify Spot Your train module should display on screen
	   		spot_your_train();
	   		//Test case 14-Verify Spot your train should be clickable
	   		spot_your_train_clickable();
	   		//Test Case 15-Verify User should click on Spot you train 
	   		spot_your_train_click();
	   		//Test case 16-Verify User navigates to next screen after click on spot your train
	   		spot_your_train_click_navigation();
	   	//Test case 17-Verify user select Train Instances
	   		Train_Instances_select();
	   		//Test case 18-Verify user select Journey Station Basis
	   		Journey_Station_Basis_select();
	   		//Test case 19-Verify user should enter train number in Train Number/Name option
	   		input_train_num();
	   		//Test case 20-Verify after enter correct Train number , auto search should work
	   		train_auto_search();
	   		//Test case 21-Verify Train name should display as JBP- IND EXP for train number 22192
	   		Train_name();
	   		//Test case 22-Verify Default date display on screen should be current date 
	   		Date_current();
	   		//Test case 23-Verify user should select Station dropdown button
	   		Station_dropdown();
	   		//Test case 24-Verify after click on station dropdown , Station name list should display 
	   		Station_list();
	   		//Test case 25-Verify User select Dewas as Station from list
	   		Station_dewas();
	   		//Test case 26-Verify after select station from dropdown , List should close 
	   		list_close();
	   		//Test case 27-Verify station display as selected station
	   		selected_station();
	   		//Test case 28-Verify Show status button should display on screen 
	   		show_status_btn();
	   		//Test case 29-Verify Show status button should be clickable
	   		show_status_click_function();
	   		//Test case 30-Verify User Should click on Show status 
	   		show_status_click();
	   		//Test case 31-Verify user should navigates to next screen after click on show status
	   		page_navigation();
	   		//Test case 32-Verify Train number display on next screen should be correct 
	   		check_train_no();
	   		//Test case 33-Verify train name display on screen should be correct
	   		check_name();
	   		//Test case 34-Verify 1St station name should display on screen
	   		check_first_station();
	   		//Test case 35-Verify last station name should display on screen
	   		check_last_station();
	   		//Test case 36-Verify platform number should not display on screen for running train
	   		platform_no();
	   		//Test case 37-Verify For searched train arrival details should display 
	   		arrival();
	   		//Test case 38-Verify for selected train departure details should display 
	   		departure();
	   		//Test case 39-Verify Arrival time should display 
	   		arrival_time();
	   		//Test case 40-Verify Arrival status details should display
	   		arrival_update();
	   		//Test case 41-Verify Departure time should display
	   		departure_time();
	   		//Test case 42-Verify Departure status should display 
	   		departure_status();
	   		//Test case 43-Verify Last update should display on screen
	   		last_update();
	   		//Test case 44-Verify user can read last update details
	   		last_update_details();
	   		//Test case 45-Verify Refresh icon should display to refresh the train status
	   		refresh();
	   	    //Test case 46-Verify Refresh button should be clickable
	   		refresh_action();
	   		//Test case 47-Verify user should click on refresh button
	   		refresh_click();
	   		//Test case 48-Verify to add train to favorites , button should display 
	   		fav();
	     	//Test case 49-Verify favorites  button should be clickable
	   		fav_action();
	    	//Test case 50-Verify user should click on favorites button
	   		fav_click();
	   		//Test case 51-Verify More option icon should display 
	   		more_option();
	   	   //Test case 52-Verify More option button should be clickable
	   		more_option_action();
	    	//Test case 53-Verify user should click on More option button
	   		more_option_click();
	   		//Test case 54-Verify More option should open dropdown 
	   		more_dropdown();
	   	    //Test case 55-Verify More option title should be select Map
	   		more_title();
	   		//Test case 56-Verify Google option should display on list
	   		more_google();
	   	    //Test case 57-Verify Bhuvan option should display on list
	   		more_Bhuvan();
	   	    //Test case 58-Verify By default google option should be selected
	   		more_selected();
	   	    //Test case 59-Verify Bhuvan option should be clickable in order to change map
	   		bhuvan_check();
	   		//Test case 60-Verify Google option should be clickable in order to change map
	   		Google_check();
	   		//Test case 61-Verify user should change Map type to Bhuvan
	   		Bhuvan_click();
	   		//Test case 62-Verify user should change Map type to Google
	   		Google_click();
	   		//Test case 63-Verify after click on any map type dropdown should close
	   		more_close();
	   		//Test case 64-Verify after dropdown close , user should naviagte back to spot your train screen
	   		navigation_more();
}
	 
	private static void navigation_more() {
		// TODO Auto-generated method stub
		try
		{
		Thread.sleep(3000);
		boolean close=driver.findElement(By.id("cris.icms.ntes:id/arrhead")).isDisplayed();
		boolean expected=true;
		if(close==expected)
		{
			System.out.println("Test case 64 Pass-User naviagtes to spot your train");
		}
		else
		{
			System.out.println("Test case 64 Fail-User not naviagtes to spot your train");
		}
		}
		catch(Exception e)
		{
			System.out.println("Test case 64 Fail-User not naviagtes to spot your train");
		}
	}

	private static void more_close() {
		// TODO Auto-generated method stub
